  "scripts": {
    "start": "webpack-dev-server",
    "server:build": "npm run build && browser-sync start -s $npm_package_config_buildDir",
    "prettier": "prettier '**/*.{js,jsx,json,scss,md}' --write",
    "lint": "run-p lint:*",
    "lint:js": "eslint '{src,test}/**/*.{js,jsx}'",
    "lint:sass": "stylelint 'src/**/*.scss'",
    "test": "jest --config jest.config.json",
    "coverage": "jest --config jest.config.json --coverage",
    "e2e": "wdio run test/e2e/wdio.conf.js",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "prepare": "husky install"
